<h1> New Item</h1>
<%= form_with url: "/merchants/#{@merchant.id}/merchant_items/", method: :post, local: true do |f| %>
<%= f.label :Name %>
<%= f.text_field :name %>
<%= f.label :Description %>
<%= f.text_field :description %>
<%= f.label :price %>
<%= f.number_field :unit_price %>
<%= f.submit "Submit" %>
<% end %>